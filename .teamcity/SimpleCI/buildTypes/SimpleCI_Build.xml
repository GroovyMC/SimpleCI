<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Build</name>
  <description />
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="Configure TeamCity information" type="gradle-runner">
        <parameters>
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRunner.gradle.tasks.names" value="configureTeamCity" />
          <param name="ui.gradleRunner.gradle.wrapper.useWrapper" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="Build Gradle Project" type="gradle-runner">
        <parameters>
          <param name="jvmArgs" value="-Xmx1G" />
          <param name="teamcity.step.mode" value="default" />
          <param name="ui.gradleRunner.gradle.tasks.names" value="clean publish" />
          <param name="ui.gradleRunner.gradle.wrapper.useWrapper" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="SimpleCI_HttpsGithubComGroovyMCSimpleCIRefsHeadsMaster" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:*" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
          <param name="triggerRules" value="-:comment=\[noci]:**" />
        </parameters>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="BUILD_EXT_1" type="swabra">
        <parameters>
          <param name="swabra.enabled" value="swabra.after.build" />
          <param name="swabra.processes" value="kill" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_2" type="commit-status-publisher">
        <parameters>
          <param name="github_authentication_type" value="token" />
          <param name="github_host" value="https://api.github.com" />
          <param name="publisherId" value="githubStatusPublisher" />
          <param name="secure:github_access_token" value="zxxe884e7f3070f50267012a450c54af485846e30fbf95a256232e956cd1e9bea9d" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

